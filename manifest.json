{
  "manifest_version": 2,
  "name": "Video Bookmarks",
  "version": "0.0.0.0001",
  "version_name": "dev_build 0001",

  "description": "Add bookmarks to YouTube videos!",

  "browser_action": {
    "default_title": "Video Bookmarks Menu",
    "default_popup": "popup.html"
  },

  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "minimum_chrome_version": "40",

  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },

  "permissions": [
    "contextMenus",
    "tabs",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*"
  ],

  "web_accessible_resources": [
  ]
}
